      /* Header */
      header { width: 100%; height: 95px; margin: 0 auto; padding: 15px; background: #003366; box-sizing: border-box;}
      header:before, header:after { display: none;}
      header .logo { height: 65px; float: left;}
      header .logo svg { width: 215px; height: 65px;}
      header .tagline { height: 65px; color: #FFFFFF; font-size: 12px; line-height: 65px; float: right;}
      /* Containter and Default */
      .container { max-width: 100%;}
      .default { width: 100%; margin: 0 auto; padding: 2em 0; background: url('/media/11631174/vu-liveball-g-sixsigma.jpg'), #FFF; background-size: 550px auto; background-position: -25px 0; background-repeat: no-repeat; position: relative; box-sizing: border-box;}
      /* Lead */
      /* Primary, Secondary and Tail */
      .primary, .secondary, .tail { max-width: 550px; margin: 0 auto;}
      .primary { margin-top: 150px; -webkit-transition: all 0.4s ease-in-out; -moz-transition: all 0.4s ease-in-out; -ms-transition: all 0.4s ease-in-out; -o-transition: all 0.4s ease-in-out; transition: all 0.2s ease-in-out;}
      .primary h1 { font-size: 1.5em;}
      .primary h2 { font-size: 1.5em;}
      .primaryHead { max-width: 100%; margin: 2em 0 0 auto; padding: 1.25em 2em; background: #003366; color: #FFFFFF; text-align: left;}
      .primaryContent, .tailContent { padding: 0 2em; text-align: left;}
      .tailNote { font-size: 0.85em; text-align: left;}
      /* Form */
      form#LeadSystem div#field5283:before, form#LeadSystem div#field5283:after, form#LeadSystem div#field5283 select { display: none;}
      .formContainer { max-width: 550px; margin: 0 auto 2em auto; padding: 2em; background: #EFEFEF; box-shadow: 0 11px 5px -6px rgba(0, 0, 0, 0.2); text-align: left;}
      .formContainer .steps-indicator > div:first-child { display: none;}
      .formContainer .steps-indicator .progress { height: 5px; margin: 2em 0; background: rgba(0,0,0,0.05); border: none; border-radius: 5px;}
      .formContainer .steps-indicator .bar { background-color: #003366; border-radius: 5px 0 0 5px; position: relative; -webkit-transition: all 0.8s ease-in-out; -moz-transition: all 0.8s ease-in-out; -ms-transition: all 0.8s ease-in-out; -o-transition: all 0.8s ease-in-out; transition: all 0.8s ease-in-out;}
      .formContainer .steps-indicator .bar:after { content: ''; width: 8px; height: 8px; background: #FFFFFF; border: 1px solid rgba(0,51,102,0.25); border-radius: 100%; box-shadow: rgba(0,51,102,0.75) 0px 0px 10px; position: absolute; top: -2px; right: -4px; display: block;}
      .required-note { font-size: 0.95em;}
      .formContainer .selectfield::before { min-height: 1em;}
      .formContainer .selectfield::after { top: 1em; right: 1em; bottom: 0;}
      .formContainer button, .leadsystem input[type="button"] { height: 50px; margin: 1em auto 0 auto; padding: 15px 25px; background: #89CE01; border: 1px solid #FFFFFF !important; box-shadow: none; color: #FFFFFF; font-size: 1.75em; font-weight: normal; line-height: .95em; text-transform: uppercase; text-shadow: none; display: block;}
      .formContainer button:hover, .leadsystem input[type="button"]:hover { text-shadow: none;}
      .leadsystem .prevbutton[type="button"] { display: none;}
      .selectfield:before { padding: 0.5em 0.5em;}
      @-moz-document url-prefix() {
      .selectfield:before { padding: 0.64em 0.5em;}
      }
      /* Footer */
      footer { margin: 0 auto; padding: 2em;}
      footer .container { box-sizing: border-box;}
      /* Extras */
      .bat-step-2, .bat-step-3, .bat-thankyou, .militaryOpt, .lean-sixsigma, .healthcare, .financial-services, .information-tech, .sixsigma, .government { display: none;}
