      /* MULTI-STEP FORM FEEDBACK
      ===================================== */
      .steps-indicator, .required-note, .progress {
      display: none !important;
      }
      .leadsystem button, .leadsystem input[type="button"] {
      background: #D31245;
      color: #fff;
      font-weight: bold;
      border: 0;
      padding: 0.7em;
      width: 49.4%;
      text-transform: uppercase;
      }
      .feedback-container {
      position: relative;
      width: 100%;
      margin-bottom: 5em;
      }
      .step-one, .step-two, .step-three {width: 23.3%;}
      .step-one {
      position: relative;
      background: #fff;
      color: #333;
      padding: 0.5em 1em;
      height: 20px;
      float: left;
      text-align: center;
      margin: 0px 7px 0px 0px;
      z-index: 3;
      }
      .step-one:after,.step-two:after {
      left: 100%;
      top: 50%;
      border: solid transparent;
      content: "";
      height: 0;
      width: 0;
      position: absolute;
      pointer-events: none;
      border-left-color: #fff;
      border-width: 16.5px;
      margin-top: -16px;
      }
      .step-one.changed:after,.step-two.changed:after {
      left: 100%;
      top: 50%;
      border: solid transparent;
      content: "";
      height: 0;
      width: 0;
      position: absolute;
      pointer-events: none;
      border-left-color: #ca1142;
      border-width: 16.5px;
      margin-top: -16px;
      }
      .step-two{
      position: relative;
      background: #fff;
      color: #333;
      padding: 0.5em 1em;
      height: 20px;
      float: left;
      text-align: center;
      margin: 0px -9.5px 0px 0px;
      z-index: 2;
      }
      .step-two:before, .step-three:before {
      left: 0%;
      top: 50%;
      border: solid transparent;
      content: " ";
      height: 0;
      width: 0;
      position: absolute;
      pointer-events: none;
      border-left-color: #002D6A;
      border-width: 18px;
      margin-top: -18px;
      }
      .step-three {
      position: relative;
      background: #fff;
      color: #333;
      padding: 0.5em 1em;
      height: 20px;
      float: left;
      left: 18px;
      text-align: center;
      }
